<Window x:Class="Uppgift_1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Uppgift_1"
        mc:Ignorable="d"
        Title="MainWindow" Height="380" Width="300"
        Background="#EEE">
        <StackPanel>
        <Label> Ceasarkrypto </Label>

        <Label> Ange en text </Label>
        <TextBox Name="Rutatext" Margin="5" Height="50" TextWrapping="Wrap" />
        
        <Label> Ange en nyckel (1-9) </Label>
        <TextBox Name="Rutanyckel" Margin="5" Height="20" />

        <Button Click="ClickKör">ok </Button>
        <Label>Texten krypterad </Label>
        <TextBox Name="Rutakrypterad" Height="50" TextWrapping="Wrap" />
        
        </StackPanel>
</Window>
